<h1>@ViewBag.title</h1>

<h3>@ViewBag.studies.Count Result(s)</h3>

@{
    // TODO #3 - Use the same list display from Views/Search/Index.cshtml
}

@foreach (Dictionary<string, string> study in ViewBag.studies)
{
    <table class="study-listing">
        @foreach (KeyValuePair<string, string> study_fields in study)
        {
            <tr>
                <td>@study_fields.Key</td>
                <td>@study_fields.Value</td>
            </tr>

        }
    </table>

}